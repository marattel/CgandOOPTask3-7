public T subtract(T v2) {
        if (this.getDimension() == v2.getDimension()){
            float[] v1Data = this.getData();
            float[] v2Data = v2.getData();
            float[] resultData = new float[v1Data.length];
            for (int i = 0; i < v1Data.length; i++){
                resultData[i] = v1Data[i] - v2Data[i];
            }
            return createNew(resultData);
        } else {
            throw new IllegalArgumentException("Размерности векторов не совпадают");
        }
    }


    public T multiplyByScalar(float scalar) {
        float[] v1Data = this.getData();
        float[] resultData = new float[v1Data.length];
        for (int i = 0; i < v1Data.length; i++){
            resultData[i] = v1Data[i] * scalar;
        }
        return createNew(resultData);
    }


    public T divisionByScalar(float scalar) {
        if (Math.abs(scalar) > eps) {
            float[] v1Data = this.getData();
            float[] resultData = new float[v1Data.length];
            for (int i = 0; i < v1Data.length; i++) {
                resultData[i] = v1Data[i] / scalar;
            }
            return createNew(resultData);
        }else {
            throw new IllegalArgumentException("Скаляр равен нулю, а вы на него делите!");
        }
    }


    public T normalization() {
        float lengthVector = this.getLength();
        if (Math.abs(lengthVector) > eps) {
            float[] vData = this.getData();
            float[] resultData = new float[vData.length];
            for (int i = 0; i < vData.length; i++) {
                resultData[i] = vData[i] / lengthVector;
            }
            return createNew(resultData);
        } else {
            throw new IllegalArgumentException("Длина вектора равена нулю, а мы на нее делим!");
        }
    }


    public float dotProduct(T v2) {
        float result = 0;
        float[] v2Data = v2.getData();
        if (this.getDimension() == v2.getDimension()) {
            for (int i = 0; i < this.data.length; i++) {
                result += this.data[i] * v2Data[i];
            }
            return result;
        }else {
            throw new IllegalArgumentException("Размерности векторов не совпадают");
        }
    }


    public float getLength() {
        float result = 0;
        for (int i = 0; i < this.data.length; i++){
            result += this.data[i] * this.data[i];
        }
        return (float) Math.sqrt(result);
    }


    @Override
    public boolean equals(Object obj){
        if (this == obj){
            return true;
        }
        if (obj == null || obj.getClass() != this.getClass()) {
            return false;
        }else {
            Vector other = (Vector) obj;
            if (Arrays.equals(other.getData(), this.getData())){
                return true;
            }
        }
        return false;
    }


    @Override
    public String toString(){
        return Arrays.toString(this.data);
    }